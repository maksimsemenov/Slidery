    "clean-webpack-plugin": "^0.1.18",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.1",
    "eslint-plugin-jest": "^21.15.0",
    "jest": "^22.4.2",
    "less": "^3.0.1",
    "less-loader": "^4.0.6",
    "postcss-load-config": "^1.2.0",
    "postcss-loader": "^2.1.1",
    "prettier": "^1.11.1",
